      "cell_type": "code",
      "metadata": {
        "id": "7y3NizqI6Nn8"
      },
      "source": [
        "# 학습시간 대비 합격 데이터\n",
        "pass_time = [8, 9, 9, 9.5, 10, 12, 14, 14.5, 15, 16, 16, 16.5, 17, 17, 17, 17.5, 20, 20, 20]\n",
        "fail_time = [1, 2, 2.1, 2.6, 2.7, 2.8, 2.9, 3, 3.2, 3.4, 3.5, 3.6, 3, 5, 5.2, 5.4]"
