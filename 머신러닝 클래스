    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "y7Bxskl4KCI7",
        "outputId": "227fbb6d-5ce3-4989-9345-879ac7e0989f"
      },
      "source": [
        "def softmax(a):\n",
        "  exp_a = np.exp(a)         # 지수함수\n",
        "  sum_exp_a = np.sum(exp_a) # 지수의 합\n",
        "  y = exp_a / sum_exp_a     # 확률\n",
        "  return y\n",
        "\n",
        "\n",
        "a = np.array([0.3, 2.9, 4.0])\n",
        "y = softmax(a)\n",
        "print(y)\n",
        "print( np.sum(y) )"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "[0.01821127 0.24519181 0.73659691]\n",
            "1.0\n"
          ],
          "name": "stdout"
        }
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "HN9HZuqZLeXF",
        "outputId": "88e337ea-ab72-4bc0-c6f5-96c4eb70d75e"
      },
      "source": [
        "# 오버플로우 대책\n",
        "def softmax(a):\n",
        "  c = np.max(a)\n",
        "  exp_a = np.exp(a - c)     # 지수함수\n",
        "  y = exp_a / np.sum(exp_a) # 지수의 합, 확률\n",
        "  return y\n",
        "\n",
        "\n",
        "a = np.array([1010, 1000, 990])\n",
        "y = softmax(a)\n",
        "print(y)\n",
        "print( np.sum(y) )"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "[9.99954600e-01 4.53978686e-05 2.06106005e-09]\n",
            "1.0\n"
          ],
          "name": "stdout"
        }
      ]
    }
  ]
}