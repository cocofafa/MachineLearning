      "source": [
        "# 모델시각화\n",
        "def logreg(z):\n",
        "  return 1 / (1 + np.exp(-z))\n",
        "\n",
        "xx = np.linspace(1, 21, 100)\n",
        "yy = logreg(model.coef_ * xx + model.intercept_)[0]\n",
        "plt.plot(xx, yy, c='r')\n",
        "plt.scatter(X, y)\n",
        "plt.grid()"
      ],
      "execution_count": null,
      "outputs": [
