      "source": [
        "w_list = [0.8]             # 가중치\n",
        "b_list = [-2, 0, 2]        # 편향\n",
        "xx = np.linspace(-10, 10, 100)\n",
        "for w in w_list:\n",
        "  for b in b_list:\n",
        "    yy = logreg(w * xx + b)\n",
        "    plt.plot(xx, yy, label=f'{b}')\n",
        "  \n",
        "plt.legend()"
      ],
      "execution_count": null,
